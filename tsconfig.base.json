{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "lib": ["ES2020"],
      "jsx": "react-jsx",
      "allowJs": false,
      "strict": true,
      "noEmit": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@dealer/shared": ["packages/shared/src/index.ts"],
        "@dealer/shared/*": ["packages/shared/src/*"]
      }
    }
  }
  